{
  "name": "xplorer",
  "version": "3.0.1",
  "description": "Explore and share. Highly-configurable directory listing made with nodejs",
  "main": "index.js",
  "bin": {
    "explorer": "./bin/explorer"
  },
  "keywords": [
    "explorer",
    "xplorer",
    "directory listing",
    "tree",
    "listing",
    "files"
  ],
  "scripts": {
    "test": "mocha -b --harmony",
    "coverage": "istanbul cover _mocha -- --harmony",
    "postinstall": "bash scripts/postinstall.sh",
    "docs": "apidoc -i routes -o doc/api",
    "start": "node --harmony index.js"
  },
  "engines": {
    "node": ">4.0"
  },
  "apps": [
    {
      "script": "index.js",
      "node_args": "--harmony",
      "env": {
        "DEBUG": ""
      }
    }
  ],
  "apidoc": {
    "title": "Explorer api"
  },
  "repository": "http://github.com/soyuka/explorer",
  "author": "soyuka",
  "license": "MIT",
  "dependencies": {
    "archiver": "^0.14.4",
    "bcrypt": "^0.8.5",
    "bluebird": "^3.0.6",
    "body-parser": "^1.13.3",
    "connect-flash": "^0.1.1",
    "content-disposition": "^0.5.0",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "express-session": "^1.11.3",
    "faye": "^1.1.2",
    "filenamify": "^1.2.0",
    "got": "^4.2.0",
    "gulp": "^3.9.0",
    "hamljs": "^0.6.2",
    "ioredis": "^1.9.1",
    "is-glob": "^2.0.0",
    "memory-cache": "^0.1.4",
    "method-override": "^2.3.5",
    "micromatch": "^2.2.0",
    "mime": "^1.3.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.6",
    "morgan": "^1.6.1",
    "multer": "^1.0.3",
    "pretty-bytes": "^2.0.1",
    "promise-spawner": "^1.1.3",
    "relieve": "^1.0.2",
    "rss": "^1.1.1",
    "vinyl": "^1.1.0",
    "yamljs": "^0.2.3",
    "rimraf": "^2.4.2"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "gulp-add-src": "^0.2.0",
    "gulp-concat": "^2.6.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.4",
    "gulp-uglify": "^1.4.1",
    "methods": "^1.1.1",
    "mocha": "^2.2.5",
    "superagent": "^1.3.0",
    "supertest": "^1.0.1",
    "async": "^1.4.2"
  },
  "optionalDependencies": {
    "hiredis": "^0.4.1"
  }
}
